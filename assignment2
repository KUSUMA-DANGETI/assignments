
#include<stdio.h>
int main()
{
    int arr[]={4,5,1,2,9,0,7};
    int i,n=7,count=0;
    for(i=0;i<n;i++)
    {
        if(arr[i]>0)
        {
            arr[count++]=arr[i];
        }
    }
    while(count<n)
    {
        arr[count++]=0;
    }
    for(i=0;i<n;i++)
    {
        printf("%d\n",arr[i]);
    }
    return 0;
}
